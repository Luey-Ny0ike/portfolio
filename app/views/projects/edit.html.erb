<%= form_for [@skill, @project] do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %>
   <%= f.label :description %>
   <%= f.text_field :description %>
   <%= f.label :link %>
   <%= f.text_field :link %>
   <%= f.label :image %>
   <%= f.text_field :image %>
